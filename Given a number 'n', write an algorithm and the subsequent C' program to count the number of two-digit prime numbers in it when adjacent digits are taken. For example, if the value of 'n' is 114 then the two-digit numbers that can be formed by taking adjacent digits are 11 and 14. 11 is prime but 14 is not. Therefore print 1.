#include <stdio.h>
#include <stdbool.h>
#include <math.h>
bool isPrime(int num) {
    if (num <= 1) return false;
    if (num <= 3) return true;
    if (num % 2 == 0 || num % 3 == 0) return false;
    for (int i = 5; i * i <= num; i += 6) {
        if (num % i == 0 || num % (i + 2) == 0) return false;
    }
    return true;
}

int countTwoDigitPrimes(int n) {
    int count = 0;
    char str[20]; // Buffer to hold the number as a string

    // Convert the number to a string
    sprintf(str, "%d", n);

    // Iterate over adjacent pairs of digits
    for (int i = 0; str[i + 1] != '\0'; i++) {
        // Form a two-digit number
        int num = (str[i] - '0') * 10 + (str[i + 1] - '0');
        
        // Check if the number is a two-digit prime number
        if (num >= 10 && num <= 99 && isPrime(num)) {
            count++;
        }
    }

    return count;
}

int main() {
    int n;

    // Input the number
    printf("Enter a number: ");
    scanf("%d", &n);
    int result = countTwoDigitPrimes(n);
    printf("Number of two-digit prime numbers: %d\n", result);

    return 0;
}
